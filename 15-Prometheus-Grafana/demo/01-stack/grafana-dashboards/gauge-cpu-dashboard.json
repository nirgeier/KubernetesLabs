    "uid": "gauge-cpu-demo",
    "title": "CPU Gauge - Demo",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "type": "gauge",
        "title": "Max CPU Usage (pods)",
