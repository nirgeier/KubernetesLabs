{
  "dashboard": {
    "id": null,
    "uid": "node-metrics-demo",
    "title": "Node Metrics - Demo",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "15s",
    "panels": [
      {
        "type": "graph",
        "title": "Node CPU Usage",
        "targets": [
          {
            "expr": "sum(rate(node_cpu_seconds_total{mode!~\"idle|iowait\"}[5m])) by (instance)",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "type": "graph",
        "title": "Node Memory Usage",
        "targets": [
          {
            "expr": "node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes",
            "refId": "A"
          }
        ],
        "datasource": "Prometheus",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      }
    ]
  },
  "overwrite": true
}